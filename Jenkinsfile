pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Welcome to the project'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying the project'
            }
        }
        stage('Testing') {
            steps {
                echo 'Pushed to testing'
            }
        }
        stage('Production') {
            steps {
                echo 'Released for production'
            }
        }
    }
}
